<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SchoolInformationDesk.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SchoolInformationDesk"
    Shell.FlyoutBehavior="Flyout"
    Title="SchoolInformationDesk">

    <ShellContent
        Title="    Информационная страница"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="    Расписание"
        ContentTemplate="{DataTemplate local:Shedule}"
        Route="Shedule" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
